{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:siyuan": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "../../dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:siyuan": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "cache": {
      "cache": false
    },
    "docs:dev": {
      "cache": false,
      "persistent": true
    },
    "docs:build": {
      "outputs": [
        ".vitepress/dist/**"
      ]
    },
    "docs:preview": {
      "cache": false,
      "persistent": true
    }
  }
}