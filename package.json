{
  "name": "sy-post-publisher",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "vite --port 6006",
    "vdev": "vercel dev --listen 6006",
    "build": "vue-tsc --noEmit && vite build",
    "build-siyuan": "vue-tsc --noEmit && BUILD_TYPE=siyuan vite build",
    "preview": "vite preview",
    "test": "vitest --watch",
    "coverage": "vitest run --coverage",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json",
    "widget": "bash scripts/widget.sh",
    "extChrome": "bash scripts/ext.sh",
    "extChromeDev": "bash scripts/extDev.sh",
    "extChromeDevWin": "pwsh scripts/extDevWin.ps1",
    "extFirefox": "bash scripts/extFV2.sh",
    "w": "yarn widget",
    "e": "yarn extChrome",
    "edev": "yarn extChromeDev",
    "edw": "yarn extChromeDevWin",
    "ef2": "yarn extFirefox"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@octokit/core": "^4.0.4",
    "@vueuse/core": "^9.6.0",
    "copy-to-clipboard": "^3.3.1",
    "cross-fetch": "^3.1.5",
    "element-plus": "^2.2.11",
    "express": "^4.18.1",
    "fast-xml-parser": "^4.0.10",
    "happy-dom": "^7.7.0",
    "highlight.js": "^11.7.0",
    "js-base64": "^3.7.2",
    "js-yaml": "^4.1.0",
    "pinia": "^2.0.25",
    "plantuml-encoder": "^1.4.0",
    "shorthash2": "^1.0.3",
    "transliteration": "^2.3.5",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "xmlrpc": "^1.3.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@types/express": "^4.17.13",
    "@types/node": "^16.18.1",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vue/test-utils": "^2.2.4",
    "node-localstorage": "^2.2.1",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.22.11",
    "vercel": "^28.5.6",
    "vite": "^3.0.0",
    "vite-plugin-mpa": "^1.1.4",
    "vite-plugin-require-transform": "^1.0.3",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
  }
}
