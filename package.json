{
  "name": "siyuan-plugin-publisher-new-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:siyuan": "turbo run dev:siyuan",
    "build:siyuan": "turbo run build:siyuan",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "./scripts/clean.sh",
    "makeLink": "node scripts/make_dev_link.js",
    "docs:dev": "turbo run --filter=@siyuan-plugin-publisher/docs dev",
    "docs:build": "turbo run --filter=@siyuan-plugin-publisher/docs build",
    "docs:preview": "turbo run --filter=@siyuan-plugin-publisher/docs preview"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.11.0"
}