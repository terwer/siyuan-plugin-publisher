.tg-app-shell
  display flex
  width 100%
  height 100%
  background-color var(--tg-color-bg)
  border-radius $tg-border-radius-lg
  box-shadow 0 2px 8px var(--tg-color-shadow)
  position relative

  &.fixed
    position fixed
    top 0
    left 0
    right 0
    bottom 0
    z-index 1000

  &__nav
    transition width 0.3s ease, border 0.3s ease, opacity 0.3s ease
    border-right 1px solid var(--tg-color-border)
    position absolute
    left 0
    top 0
    bottom 0
    height 100%
    flex-shrink 0
    z-index 10
    background-color var(--tg-color-bg)

    &.tg-app-shell__nav--collapsed
      width 0 !important
      opacity 0
      visibility hidden
      border-right-color transparent

  &__nav-header
    padding $tg-spacing-md $tg-spacing-lg

  &__nav-content
    height 100%
    transition width 0.3s ease

  &__nav-item
    height 48px
    padding 0 $tg-spacing-md
    display flex
    align-items center
    color var(--tg-color-text-2)
    cursor pointer
    transition all 0.3s ease

    &:hover
      background-color var(--tg-color-bg-hover)
      color var(--tg-color-text-1)

    &--active
      background-color var(--tg-color-bg-hover)
      color var(--tg-color-primary)

  &__nav-icon
    font-size $tg-font-size-lg
    margin-right $tg-spacing-md

  &__collapse-handle
    position absolute
    top 50%
    right -12px
    transform translateY(-50%)
    width 24px
    height 24px
    background-color var(--tg-color-bg)
    border 1px solid var(--tg-color-border)
    border-radius 50%
    display flex
    align-items center
    justify-content center
    cursor pointer
    z-index 1
    transition all 0.3s ease

    &:hover
      background-color var(--tg-color-bg-hover)

  &__main
    flex 1
    display flex
    flex-direction column
    overflow hidden

  &__header
    padding $tg-spacing-md $tg-spacing-lg

  &__content
    flex 1
    min-width 0
    height 100%
    background-color var(--tg-color-bg)
    padding $tg-spacing-sm $tg-spacing-md
    display flex
    flex-direction column
    overflow hidden
    transition margin-left 0.3s ease

    &--collapsed
      margin-left 0

  &__content-item
    flex 1
    overflow-y auto

  &__content-item-header
    height 64px
    padding 0 $tg-spacing-lg
    display flex
    align-items center
    justify-content space-between
    border-bottom 1px solid var(--tg-color-border)
    background-color var(--tg-color-bg)

  &__content-item-header-title
    font-size $tg-font-size-lg
    font-weight bold

  &__content-item-header-actions
    display flex
    align-items center

  &__content-item-header-action
    margin-left $tg-spacing-md
    cursor pointer

  &__content-item-header-action-icon
    font-size $tg-font-size-lg

  &__content-item-body
    padding $tg-spacing-lg
    overflow-y auto

  &__content-item-body-title
    font-size $tg-font-size-lg
    font-weight bold
    margin-bottom $tg-spacing-md

  &__content-item-body-description
    color var(--tg-color-text-2)

  &__content-item-body-actions
    margin-top $tg-spacing-lg
    display flex
    justify-content space-between

  &__content-item-body-action
    cursor pointer

  &__content-item-body-action-icon
    font-size $tg-font-size-lg
    margin-left $tg-spacing-md

